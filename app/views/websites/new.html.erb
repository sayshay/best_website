
    <!-- svg иконки end-->
    <div class="page-wrapp">
      <div class="content-without-footer">
        <%= render 'layouts/header' %>
        <!-- submit start-->
        <div class="submit-wrap">
          <div class="container">
            <div class="submit-wrap_title"><span>SUBMIT A <b>WEBSITE</b></span></div>
            <div class="submit-wrap_af-title-text">
              <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Duis posuere cursus rutrum. Duis orci metus, lobortis ut consectetur non, elementum non elit. Praesent viverra quam sit amet lacinia aliquet. Ut interdum tortor non quam mollis vestibulum. Maecenas at magna sapien</p>
            </div>
            <div class="submit-wrap_form-wrap">
              <div class="submit-wrap_form-wrap_title"><span>INFO ABOUT WEBSITE TO BE SUBMITTED</span></div>
              <%= form_for @website do |f|%>
                <%= f.hidden_field :user_id, value: @user.id  %>
                <div class="submit-wrap_form-wrap_line clearfix">
                  <div class="submit-wrap_form-wrap_line_text"><span>COMPANY NAME</span></div>
                  <div class="submit-wrap_form-wrap_line_input">
                    <div class="input-wrap">
                      <%= f.text_field :name, placeholder: "Site Name", required: true%>

                    </div>
                  </div>
                </div>
                <div class="submit-wrap_form-wrap_line clearfix">
                  <div class="submit-wrap_form-wrap_line_text"><span>WEBSITE PAGE URLS</span></div>
                  <div class="submit-wrap_form-wrap_line_input">
                    <div class="submit-wrap_form-wrap_line clearfix">
                      <div class="submit-wrap_form-wrap_line_input_page-name">
                        <div class="input-wrap">
                          <input type="text" placeholder="Page Name 1">
                        </div>
                      </div>
                      <div class="submit-wrap_form-wrap_line_input_page-url">
                        <div class="submit-wrap_form-wrap_line_input_page-url_http"><span>http://</span></div>
                        <div class="input-wrap">
                          <input type="text" placeholder="Page url">
                        </div>
                      </div>
                    </div>
                    <div class="submit-wrap_form-wrap_line clearfix">
                      <div class="submit-wrap_form-wrap_line_input_page-name">
                        <div class="input-wrap">
                          <input type="text" placeholder="Page Name 2">
                        </div>
                      </div>
                      <div class="submit-wrap_form-wrap_line_input_page-url">
                        <div class="submit-wrap_form-wrap_line_input_page-url_http"><span>http://</span></div>
                        <div class="input-wrap">
                          <input type="text" placeholder="Page url">
                        </div>
                      </div>
                    </div>
                    <div class="submit-wrap_form-wrap_line clearfix">
                      <div class="submit-wrap_form-wrap_line_input_page-name">
                        <div class="input-wrap">
                          <input type="text" placeholder="Page Name 3">
                        </div>
                      </div>
                      <div class="submit-wrap_form-wrap_line_input_page-url">
                        <div class="submit-wrap_form-wrap_line_input_page-url_http"><span>http://</span></div>
                        <div class="input-wrap">
                          <input type="text" placeholder="Page url">
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="submit-wrap_form-wrap_line clearfix">
                  <div class="submit-wrap_form-wrap_line_text"><span>TWITTER</span></div>
                  <div class="submit-wrap_form-wrap_line_input">
                    <div class="input-wrap">
                      <%= f.text_field :twitter, placeholder: "Twitter Handle of Website", required: true%>
                    </div>
                  </div>
                </div>
                <div class="submit-wrap_form-wrap_line clearfix">
                  <div class="submit-wrap_form-wrap_line_text"><span>Image</span></div>
                  <div class="submit-wrap_form-wrap_line_input">
                    <div class="input-wrap">
                      <%= f.file_field :image, placeholder: "Website Image", class: "inputfile", required: true %>
                      <%= f.label :image do %><svg xmlns="http://www.w3.org/2000/svg" width="20" height="17" viewBox="0 0 20 17"><path d="M10 0l-5.2 4.9h3.3v5.1h3.8v-5.1h3.3l-5.2-4.9zm9.3 11.5l-3.2-2.1h-2l3.4 2.6h-3.5c-.1 0-.2.1-.2.1l-.8 2.3h-6l-.8-2.2c-.1-.1-.1-.2-.2-.2h-3.6l3.4-2.6h-2l-3.2 2.1c-.4.3-.7 1-.6 1.5l.6 3.1c.1.5.7.9 1.2.9h16.3c.6 0 1.1-.4 1.3-.9l.6-3.1c.1-.5-.2-1.2-.7-1.5z"></path></svg>
                      <span>Choose a file...</span>
                      <% end %>
                    </div>
                  </div>
                </div>
                <div class="submit-wrap_form-wrap_line clearfix">
                  <div class="submit-wrap_form-wrap_line_text"><span>INDUSTRY </span></div>
                  <div class="submit-wrap_form-wrap_line_input">
                    <div class="input-wrap">
                      <select title="Select Industry of Website" class="selectpicker" name="website[industry]">
                        <option value="Industry 1">Industry 1</option>
                        <option value="Industry 2">Industry 2</option>
                        <option value="Industry 3">Industry 3</option>
                        <option value="Industry 4">Industry 4</option>
                        <option value="Industry 5">Industry 5</option>
                        <option value="Industry 6">Industry 6</option>
                        <option value="Industry 7">Industry 7</option>
                        <option value="Industry 8">Industry 8</option>
                      </select>

                    </div>
                  </div>
                </div>
                <div class="submit-wrap_form-wrap_line clearfix">
                  <div class="submit-wrap_form-wrap_line_text"><span>FEATURES </span></div>
                  <div class="submit-wrap_form-wrap_line_input">
                    <div class="input-wrap">
                      <%= f.text_field :features, placeholder: "3 Notable Design / Functionality Features"%>
                    </div>
                  </div>
                </div>
                <div class="submit-wrap_form-wrap_line clearfix">
                  <div class="submit-wrap_form-wrap_line_text"><span>DESCRIPTION  </span></div>
                  <div class="submit-wrap_form-wrap_line_input">
                    <div class="input-wrap">
                      <%= f.text_area :description, placeholder: "Description of Website [150 to 300 Words in Length])", required: true %>
                    </div>
                  </div>
                </div>
                <div class="submit-wrap_form-wrap_next-button clearfix">
                  <button type="submit" class="btn pull-right">NEXT</button>
                </div>
              <% end %>
            </div>
            <div class="submit-wrap_steps-wrap">
              <div class="submit-wrap_steps-wrap_inn clearfix">
                <div class="submit-wrap_steps-wrap_step active">
                  <div class="submit-wrap_steps-wrap_step_number"><span>1</span></div>
                  <div class="submit-wrap_steps-wrap_step_text"><span>SITE INFORMATION</span></div>
                </div>
                <div class="submit-wrap_steps-wrap_line"></div>
                <div class="submit-wrap_steps-wrap_step">
                  <div class="submit-wrap_steps-wrap_step_number"><span>2</span></div>
                  <div class="submit-wrap_steps-wrap_step_text"><span>DESIGNER INFORMATION</span></div>
                </div>
                <div class="submit-wrap_steps-wrap_line"></div>
                <div class="submit-wrap_steps-wrap_step">
                  <div class="submit-wrap_steps-wrap_step_number"><span>3</span></div>
                  <div class="submit-wrap_steps-wrap_step_text"><span>REVIEW AND CONFIRM</span></div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- submit end-->

